  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        tblCertificate.CertificateName, tblFilm.FilmOscarWins, tblFilm.FilmID
FROM            tblCertificate INNER JOIN
                         tblFilm ON tblCertificate.CertificateID = tblFilm.FilmCertificateID
WHERE        (tblCertificate.CertificateName &lt;&gt; 'Unknown')</CommandText>
      </Query>
      <Fields>
        <Field Name="CertificateName">
          <DataField>CertificateName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
