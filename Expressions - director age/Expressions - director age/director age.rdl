  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        tblDirector.DirectorName, tblDirector.DirectorDOB, tblFilm.FilmName, tblFilm.FilmReleaseDate, tblFilm.FilmOscarWins
FROM            tblDirector INNER JOIN
                         tblFilm ON tblDirector.DirectorID = tblFilm.FilmDirectorID
WHERE        (tblFilm.FilmOscarWins = 3)
ORDER BY tblFilm.FilmName</CommandText>
      </Query>
      <Fields>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DirectorDOB">
