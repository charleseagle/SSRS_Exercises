  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        tblCertificate.CertificateName, COUNT(tblFilm.FilmID) AS Expr1
FROM            tblCertificate INNER JOIN
                         tblFilm ON tblCertificate.CertificateID = tblFilm.FilmCertificateID
WHERE  tblCertificate.CertificateName &lt;&gt; 'Unknown'
GROUP BY tblCertificate.CertificateName
ORDER BY tblCertificate.CertificateName</CommandText>
      </Query>
      <Fields>
        <Field Name="CertificateName">
          <DataField>CertificateName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expr1">
