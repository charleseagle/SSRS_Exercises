  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        tblActor.ActorID, tblActor.ActorName, tblDirector.DirectorID, tblFilm.FilmName
FROM            tblFilm INNER JOIN
                         tblDirector ON tblFilm.FilmDirectorID = tblDirector.DirectorID INNER JOIN
                         tblActor INNER JOIN
                         tblCast ON tblActor.ActorName = tblCast.CastActorID ON tblFilm.FilmID = tblCast.CastFilmID</CommandText>
      </Query>
      <Fields>
